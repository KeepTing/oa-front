/* pages/address/staffList/staffList.wxss */
.radio{
  padding: 10px;
  display: block;
}
.boxcheck{
  width: 100%;
  height: 55px;
  line-height: 55px;
  background-color:white;
  border-bottom: 1px solid #eee;
  position: relative;
}
.checkbox{
  width: 80%;
  display: block;

}

.checkbox checkbox .wx-checkbox-input {
margin-right:5px;
-webkit-appearance:none;
appearance:none;
outline:0;
border:1px solid #D1D1D1;
background-color:#FFFFFF;
border-radius:50%;
width:22px;
height:22px;
position:relative;
margin: 0px  10px;

}
.tx{
  display: inline-block;
  width: 40px;
  height: 40px; 
  border-radius: 50%;
  background-color: #289bf0;
  font-size: 12px;
  color: white;
  line-height: 40px;
  text-align: center;
  margin: 0px 10px;
}
.btnstaff{
  display: inline-block;
  position: absolute;
  right: 10px;
  top:20px;
}
